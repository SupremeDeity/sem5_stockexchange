<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0"
   xmlns:fx="http://javafx.com/fxml/1"
   xmlns="http://javafx.com/javafx/8" fx:controller="com.stockexchangeapp.view.CurrencyFormDialogController">
   <children>
      <GridPane layoutX="116.0" layoutY="77.0" prefHeight="111.0" prefWidth="303.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Label text="Currency name" />
            <Label text="Currency code" GridPane.rowIndex="1" />
            <TextField fx:id="nameField" GridPane.columnIndex="1" />
            <TextField fx:id="codeField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="269.0" layoutY="188.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="63.0" onAction="#handleOk" text="OK" />
      <Button layoutX="350.0" layoutY="188.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
   </children>
</AnchorPane>
