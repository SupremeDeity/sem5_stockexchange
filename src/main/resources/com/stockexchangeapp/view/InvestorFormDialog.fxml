        <TextField fx:id="idField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      </children>
    </GridPane>
    <Button layoutX="350.0" layoutY="318.0" mnemonicParsing="false" onAction="#handleOk" prefHeight="25.0" prefWidth="63.0" text="OK" />
    <Button layoutX="431.0" layoutY="318.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
